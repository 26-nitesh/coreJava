package package1;

public class B {

	public static void main(String[] args) {
		
		byte b = 12;            //1byte
		short s = 1234;         // 2byte
		int i = 123456543;      //4 byte
//		Implicit type casting   --> Compiler will perform
	
//Explicit type casting -- > We have to do		
		long l = 9876543567890l; //8 byte
		
		
		
		float f1 = 12345.34f;
		float f = (float)123.6; //4 byte
		double d = 234.55; // 8 byte 
		
		char c = 'a';  // 2 byte
		
		boolean bool = true;
		
		
							
	}
}
