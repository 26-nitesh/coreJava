package package1;

public class Test {
	
	public static void greet() {
		System.out.println(" Hi, I am Greeting You");
	}
	
	public static void add1(int a, int b) {
		System.out.println("I am inside Add 1");
		int c = a+b;
		System.out.println(c);
	}

	public static int add2(int a, int b) {
		System.out.println("I am Inside Add 2");
		int c = a+b;
		return c;
		
	}

}
